  "scripts-info": {
    "start": "Serves default",
    "start:ssr": "Builds ssr and serves ssr",
    "serve:ssl": "Server local ssl"
  },
  "scripts": {
    "start": "ng serve",
    "clean:install": "run-s rimraf node_modules dist && npm i",
    "start:clean": "run-s clean:install start",
    "start:ssr": "run-s clean:install ssr:build ssr:serve",
    "ssr:build": "run-s build:client-and-server-bundles",
    "ssr:serve": "node dist/server/main.js",
    "ssr:webpack": "webpack --config webpack.server.config.js --progress --colors",
    "build": "ng build --prod --aot --buildOptimizer --optimization --deleteOutputPath ----extractCss --sourceMap=false",
    "build:gzip": "npm run build && gzip-all \"dist/browser/*.{css,css,js,html}\"",
    "format": "prettier --config .prettierrc --ignore-path .prettierignore \"**/*.{ts,json,md,css,js}\"",
    "format:check": "npm run format -- --check",
    "format:watch": "onchange '**/*.{ts,json,md,css,js}' -- prettier --config .prettierrc --ignore-path .prettierignore --write {{changed}}",
    "format:write": "npm run format -- --write",
    "lint": "ng lint",
    "stylelint": "stylelint \"src/{css,app}/**/*.css\" --config .stylelintrc --fix",
    "test": "ng test",
    "release": "standard-version",
    "pretty-quick": "pretty-quick --staged",
    "compress:brotli": "ts-node brotli_compress.ts",
    "compress:svg:src": "svgo src/assets/images/**/*.svg",
    "compress:svg:dist": "svgo dist/browser/assets/images/**/*.svg",
    "serve:ssl": "ng serve --aot --optimization --prod --ssl true --sslCert '/cert/cert.crt' --sslKey '/cert/cert.key'",
    "build:client-and-server-bundles": "ng build --prod && npm run build:gzip && ng run buffer:server:production",
    "serve:ssr": "node dist/server/main.js",
    "build:ssr": "ng build --prod && ng run buffer:server:production",
    "postinstall": "ngcc"
  },
  "private": true,
