  "repository": {
    "type": "git",
    "url": "https://github.com/abdul-baten/buffer-client.git"
  },
  "scripts": {
    "start": "ng serve",
    "start:clean": "npm run clean:install && npm run start",
    "start:prerender": "npm run prerender:build && cd dist/browser && http-server -g -P \"404\" -c 12960000",
    "start:ssr": "npm run ssr:build && npm run ssr:serve",
    "certificate:generate": "cd ci && bash generateCertificateOSX.sh",
    "clean": "rimraf node_modules reports dist coverage",
    "clean:install": "npm run clean && npm i",
    "build": "ng build --prod --stats-json",
    "build:client-and-server-bundles": "npm run build:gzip && ng run buffer:server:production",
    "build:gzip": "npm run build && gzip-all \"dist/browser/*.{scss,css,js,html}\"",
    "format": "prettier --config .prettierrc --ignore-path .prettierignore \"**/*.{ts,json,md,scss,js}\"",
    "format:check": "npm run format -- --check",
    "format:watch": "onchange '**/*.{ts,json,md,scss,js}' -- prettier --config .prettierrc --ignore-path .prettierignore --write {{changed}}",
    "format:write": "npm run format -- --write",
    "lint": "ng lint && npm run stylelint",
    "lighthouse": "node ./utils/fetchLighthouse.js",
    "prerender:build": "npm run build:client-and-server-bundles && tsc -p server.tsconfig.json && npm run prerender:generate",
    "prerender:generate": "cd dist && node prerender",
    "ssr:build": "npm run build:client-and-server-bundles && npm run ssr:webpack",
    "ssr:serve": "node dist/server.js",
    "ssr:webpack": "webpack --config webpack.server.config.js --progress --colors",
    "stylelint": "stylelint \"src/{scss,app}/**/*.scss\" --config .stylelintrc --fix",
    "test": "rimraf \"coverage\" & ng test --code-coverage --browsers Chrome",
    "test:e2e": "rimraf \"reports/e2e/{json-output-folder,report,screenshots}\" && ng e2e",
    "test:unit-headless": "npm run test -- --browsers ChromeNoSandbox --watch=false",
    "wpo:stats": "webpack-bundle-analyzer dist/browser/stats.json --mode=static --report=\"reports/wpo/bundle-report.html\" --no-open && npx cpx dist/browser/stats.json reports/wpo"
  },
  "private": true,
