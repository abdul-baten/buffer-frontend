  "scripts-info": {
    "start": "Serves default",
    "ssr": "Builds ssr and serves ssr",
    "ssl": "Server local ssl"
  },
  "scripts": {
    "start": "ng serve",
    "clean:install": "rimraf dist && npm i",
    "start:clean": "run-s clean:install start",
    "ssr": "run-s clean:install ssr:build build:gzip ssr:serve",
    "ssr:build": "run-s build:client-and-server-bundles",
    "ssr:serve": "node dist/server/main.js",
    "ssr:webpack": "webpack --config webpack.server.config.js --progress --colors",
    "ssl": "ng serve --ssl true --sslCert './cert/cert.crt' --sslKey './cert/cert.key'",
    "build": "ng build --prod",
    "build:gzip": "npm run build && gzipper compress --level 9 --memory-level 9 --strategy 4 --deflate --brotli  ./dist/browser",
    "build:client-and-server-bundles": "npm run build && ng run buffer:server:production",
    "format": "prettier --config .prettierrc --ignore-path .prettierignore \"**/*.{ts,json,md,css,js}\"",
    "format:check": "npm run format -- --check",
    "format:watch": "onchange '**/*.{ts,json,md,css,js}' -- prettier --config .prettierrc --ignore-path .prettierignore --write {{changed}}",
    "format:write": "npm run format -- --write",
    "lint": "ng lint",
    "stylelint": "stylelint \"src/**/*.css\" --config .stylelintrc --fix",
    "test": "ng test",
    "release": "standard-version",
    "pretty-quick": "pretty-quick --staged",
    "compress:brotli": "ts-node brotli_compress.ts",
    "compress:svg:src": "svgo src/assets/images/**/*.svg",
    "compress:svg:dist": "svgo dist/browser/assets/images/**/*.svg",
    "postinstall": "ngcc",
    "imageoptim": "imageoptim --imagealpha 'src/assets/**/*.png'",
    "webpack:analyze": "npx webpack-bundle-analyzer dist/browser/stats.json",
    "closure": "npx google-closure-compiler --compilation_level='WHITESPACE_ONLY' --js='dist/server/main.js' --js_output_file='dist/main.js'"
  },
  "private": true,
